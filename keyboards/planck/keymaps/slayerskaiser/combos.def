COMB(num1, KC_1, KC_Q, KC_A)
COMB(num2, KC_2, KC_W, KC_S)
COMB(num3, KC_3, KC_E, KC_D)
COMB(num4, KC_4, KC_R, KC_F)
COMB(num5, KC_5, KC_T, KC_G)
COMB(num6, KC_6, KC_Y, KC_H)
COMB(num7, KC_7, KC_U, KC_J)
COMB(num8, KC_8, KC_I, KC_K)
COMB(num9, KC_9, KC_O, KC_L)
COMB(num0, KC_0, KC_P, KC_SCOLON)

COMB(left_bracket_ez, KC_LBRACKET, KC_R, KC_T) // [
COMB(right_bracket_ez, KC_RBRACKET, KC_Y, KC_U) // ]
COMB(left_paren_ez, KC_LEFT_PAREN, KC_F, KC_G) // (
COMB(right_paren_ez, KC_RIGHT_PAREN, KC_H, KC_J) // )
COMB(left_curly_ez, KC_LCBR, KC_V, KC_B) // {
COMB(right_curly_ez, KC_RCBR, KC_N, KC_M) // }

COMB(hyphen, KC_MINUS, KC_J, KC_M) // -
COMB(equal, KC_EQUAL, KC_K, KC_COMMA) // =

